        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SeldatUnilever_Ver1._02"
        mc:Ignorable="d"
        Title="" Height="200" Width="250">
    <Grid>
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="75*"/>
            <ColumnDefinition Width="167*"/>
        </Grid.ColumnDefinitions>
        <Label Content="Please select gate need reset" HorizontalAlignment="Left" Margin="39,32,0,0" VerticalAlignment="Top" Width="171" Grid.ColumnSpan="2"/>
        <Button x:Name="cmdDoor1Reset" Grid.ColumnSpan="2" Content="Gate 1" HorizontalAlignment="Left" Margin="28,86,0,0" VerticalAlignment="Top" Width="75" Click="CmdDoor1Reset_Click"/>
        <Button x:Name="cmdDoor2Reset" Content="Gate 2" Grid.Column="1" HorizontalAlignment="Left" Margin="60,86,0,0" VerticalAlignment="Top" Width="75" Click="CmdDoor2Reset_Click"/>
        <Label Name="lblStatus" Content="" HorizontalAlignment="Center" Margin="60,131,56,0" VerticalAlignment="Top" RenderTransformOrigin="-0.395,-1.1" Width="126" Grid.ColumnSpan="2"/>

    </Grid>
</Window>
